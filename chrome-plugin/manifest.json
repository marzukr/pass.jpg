{
    "name": "pass.jpg",
    "version": "1.0",
    "description": "The simple and secure way to make a password",
    "permissions": ["activeTab", "declarativeContent", "storage"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/passicon2.png",
            "32": "images/passicon2.png",
            "48": "images/passicon2.png",
            "128": "images/passicon2.png"
        }
    },
    "icons": {
        "16": "images/passicon2.png",
        "32": "images/passicon2.png",
        "48": "images/passicon2.png",
        "128": "images/passicon2.png"
    },
    "options_page": "options.html",
    "manifest_version": 2
}